  const queryClient = useQueryClient();

  const query = useQuery({
    queryKey: ['order', id],
    queryFn: () => ordersService.getOrder(id),
    enabled: !!id,
  });

  // Real-time updates are now handled by useChatOrdersSocket hook
  // This hook focuses only on data fetching

  return query;
};

