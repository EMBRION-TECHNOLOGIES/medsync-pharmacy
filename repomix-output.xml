'use client';

import { Menu, LogOut, Shield, UserCheck, Clock, ShieldCheck, AlertTriangle, UserCog } from 'lucide-react';
import { Button } from '@/components/ui/button';
import { Badge } from '@/components/ui/badge';
import {
  DropdownMenu,
  DropdownMenuContent,
